<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScGoods" table="SC_GOODS">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="goodsName" type="java.lang.String">
            <column name="GOODS_NAME" length="36">
                <comment>商品名称</comment>
            </column>
        </property>
        <property name="goodsBrandId" type="java.lang.String">
            <column name="GOODS_BRAND_ID" length="36">
                <comment>品牌id</comment>
            </column>
        </property>
        <property name="goodsDetail" type="java.lang.String">
            <column name="GOODS_DETAIL">
                <comment>商品详情</comment>
            </column>
        </property>
        <property name="goodsIsuse" type="java.lang.String">
            <column name="GOODS_ISUSE" length="1">
                <comment>是否停用</comment>
            </column>
        </property>
        <property name="stopTime" type="java.util.Date">
            <column name="STOP_TIME" length="7">
                <comment>停用时间</comment>
            </column>
        </property>
        <property name="goodsFullname" type="java.lang.String">
            <column name="GOODS_FULLNAME" length="100">
                <comment>商品全称</comment>
            </column>
        </property>
        <property name="goodsCode" type="java.lang.String">
            <column name="GOODS_CODE" length="36">
                <comment>商品编码</comment>
            </column>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" length="7">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="createPeople" type="java.lang.String">
            <column name="CREATE_PEOPLE" length="36">
                <comment>创建人</comment>
            </column>
        </property>
        <property name="noStockSale" type="java.lang.String">
            <column name="NO_STOCK_SALE" length="1">
                <comment>无库存是否销售</comment>
            </column>
        </property>
        <property name="isshelves" type="java.lang.String">
            <column name="ISSHELVES" length="1">
                <comment>是否上架</comment>
            </column>
        </property>
        <property name="belong" type="java.lang.String">
            <column name="BELONG" length="36">
                <comment>所属</comment>
            </column>
        </property>
        <property name="isVirtualGoods" type="java.lang.String">
            <column name="IS_VIRTUAL_GOODS" length="1">
                <comment>是否虚拟商品</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
