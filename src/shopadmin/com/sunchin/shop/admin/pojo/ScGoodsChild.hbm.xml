<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sunchin.shop.admin.pojo.ScGoodsChild" table="SC_GOODS_CHILD">
        <id name="id" type="java.lang.String">
            <column name="ID" length="36" />
            <generator class="assigned"></generator>
        </id>
        <property name="goodsChildCode" type="java.lang.String">
            <column name="GOODS_CHILD_CODE" length="36">
                <comment>子商品code</comment>
            </column>
        </property>
        <property name="goodsId" type="java.lang.String">
            <column name="GOODS_ID" length="36">
                <comment>商品id</comment>
            </column>
        </property>
        <property name="goodsChildName" type="java.lang.String">
            <column name="GOODS_CHILD_NAME" length="100">
                <comment>子商品名称</comment>
            </column>
        </property>
        <property name="goodsChildMemo" type="java.lang.String">
            <column name="GOODS_CHILD_MEMO" length="100">
                <comment>子商品描述</comment>
            </column>
        </property>
        <property name="purchasePrice" type="java.lang.Double">
            <column name="PURCHASE_PRICE" precision="10">
                <comment>采购价</comment>
            </column>
        </property>
        <property name="marketPrice" type="java.lang.Double">
            <column name="MARKET_PRICE" precision="10">
                <comment>市场价</comment>
            </column>
        </property>
        <property name="salePrice" type="java.lang.Double">
            <column name="SALE_PRICE" precision="10">
                <comment>销售价</comment>
            </column>
        </property>
        <property name="promotionPrice" type="java.lang.Double">
            <column name="PROMOTION_PRICE" precision="10">
                <comment>促销价</comment>
            </column>
        </property>
        <property name="goodsNo" type="java.lang.String">
            <column name="GOODS_NO" length="36">
                <comment>货号</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
